<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="5.0" maxHeight="-Infinity" vgap="5.0"
          xmlns="http://javafx.com/javafx/11.0.1" fx:controller="main.kotlin.controller.NoteController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <ColorPicker GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <TextArea prefHeight="300.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <TextField GridPane.columnIndex="1"/>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Title"/>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Note" GridPane.rowIndex="1"/>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Color" GridPane.rowIndex="2"/>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
